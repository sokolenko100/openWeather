{
  "name": "openWeather",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-build": "\\rm -fr ./node_modules && \\rm -fr dist/* && \\rm -fr ios/build ios/Pods ios/KScoreApp.xcarchive && \\rm -fr android/build android/app/build",
    "clean-cache": "cd android && ./gradlew cleanBuildCache && cd .. && watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-_ &&rm -rf $TMPDIR/metro-bundler-cache-_ && rm -rf node_modules/ && npm cache clean --force && npm install && npm start -- --reset-cache",
    "full-reinstall": "rm -rf node_module && yarn install && cd ios && pod install && cd ..",
    "full-install": "yarn install && cd ios && pod install && cd ..",
    "build:android:apk": "react-native run-android --variant=release",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-mode-debug": "react-native run-ios --simulator=\"iPhone 15\" --mode Debug",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .test.js,.ts,.tsx",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src"
  },
